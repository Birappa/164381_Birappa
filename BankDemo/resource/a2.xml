<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>

<bean id="question" class="com.spring.assignments.Question">
<property name="questionId" value="1">
</property>
<property name="question" value="Capital of India?">
</property>
    <property name = "listAnswer">
         <list>
            <value>New Delhi</value>
         </list>
      </property>
      
       <property name = "setAnswer">
         <set>
            <value>New Delhi</value>
         </set>
      </property>
      
      <property name = "mapAnswer">
         <map>
            <entry key = "1" value = "New Delhi"/>
         </map>
      </property>
 
</bean>

</beans>