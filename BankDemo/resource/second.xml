<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
     
     <beans>
    <bean id="question" class="com.spring.second.Question">
    <property name="questionId" value="1"/>
  <property name="question" value="What is name of my colleague sitting beside me?"/>
  <property name="listAnswers">
  <list>
  <value>Patis</value>
  </list>
  </property>
  <property name="setAnswers">
  <set>
  <value>Patis</value>
  </set>
  </property>
  <property name="mapAnswers">
  <map>
  
  <entry key="1" value="Patis"/>
  </map>
  </property>
  </bean>
    </beans>